{
  "Dump something": {
    "scope": "php",
    "prefix": "dump",
    "body": [
      "dump($1);exit;",
      "$2"
    ],
    "description": "Dump output, drupal 8"
  },
  "Drupal logger": {
    "prefix": "logger",
    "body": [
      " \\Drupal::logger('$1')->error(json_encode($2));",
      "$3"
    ],
    "description": "Log output in drupal 8"
  },
  "Drupal watchdog": {
    "prefix": "watchdog",
    "body": [
      "watchdog('$1',$2 ,[],WATCHDOG_ERROR);",
      "$3"
    ],
    "description": "Log output in drupal 7"
  },
  "Dump class methods": {
    "prefix": "dcm",
    "body": [
      "dump(get_class_methods($1));exit;",
      "$3"
    ],
    "description": "Debug a variable in twitg template"
  }
}
